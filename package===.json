{
  "name": "hcau-publ",
  "description": "Include Bootstrap's source Sass and compiled JavaScript bundle via npm.",
  "version": "0.0.0",
  "private": true,
  "repository": "twbs/examples",
  "license": "MIT",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules scss/:css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "sirv --dev --no-clear --port 3000",
    "start": "npm-run-all --parallel watch server",
   
    "test": "npm-run-all css-lint css",
    "watch": "node_modules/.bin/gulp watch --development",  
    "build": "node_modules/.bin/gulp build --development",
    "clean": "rm -rf resources/*",
    "folders": "node_modules/.bin/gulp folders"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "gulp-javascript-obfuscator": "^1.1.6",
    "gulp-uglify-es": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "sass": "^1.64.1",
    "sirv-cli": "^2.0.2",
    "stylelint": "^15.10.2",
    "stylelint-config-twbs-bootstrap": "^11.0.1",
    "glob": "^7.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-copy": "^4.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-debug": "^4.0.0",
    "gulp-filter": "^7.0.0",
    "gulp-if": "^3.0.0",
    "gulp-livereload": "^4.0.2",
    "gulp-mode": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-useref-custom": "file:src/module/gulp-useref-custom/gulp-useref-custom-5.0.2.tgz",
    "gulp.spritesmith": "^6.12.1",
    "gulp.spritesmith.3x": "^1.0.9",
    "install": "^0.13.0",
    "lazypipe": "^1.0.2",
    "web-dev-server": "^3.0.22"
  }
}
